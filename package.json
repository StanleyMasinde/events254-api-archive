{
  "name": "events254",
  "version": "1.0.2",
  "private": true,
  "description": "An application for event organisers in Kenya.",
  "main": "app.js",
  "keywords": [
    "Events",
    "Meetup",
    "Kenya",
    "254",
    "opensource254"
  ],
  "author": "opensource254",
  "license": "ISC",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nuxt start --spa",
    "dev": "cross-env DEBUG=express:* nuxt",
    "dev:api": "cross-env DEBUG=express:* NODE_ENV=api nodemon api/app.js",
    "pretest": "cross-env NODE_ENV=production knex migrate:latest",
    "test": "cross-env NODE_ENV=testing TEST_DB_CLIENT=mysql2 mocha --timeout 12000 --exit",
    "posttest": "cross-env npx knex migrate:rollback --env production",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "build": "nuxt build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.5.0",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.3",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mevn-orm": "^2.2.1",
    "mevn-validator": "^2.0.1",
    "moment-timezone": "^0.5.33",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.17",
    "nodemailer-pug-engine": "^1.0.2",
    "nuxt": "^2.15.0",
    "redis": "^3.0.2",
    "sessionstore": "^0.4.8",
    "vee-validate": "^3.4.5"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.13.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "eslint": "^7.20.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "faker": "^5.4.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "stylelint": "^13.10.0"
  },
  "repository": "git+https://github.com/opensource254/events254.git",
  "bugs": {
    "url": "https://github.com/opensource254/events254/issues"
  },
  "homepage": "https://github.com/opensource254/events254#readme"
