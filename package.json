    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "express-session": "^1.17.2",
    "ical-generator": "^4.1.0",
    "mevn-orm": "^2.4.5",
    "mevn-validator": "^2.0.2",
